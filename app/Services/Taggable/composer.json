{
    "name": "imanghafoori/taggable",
    "description": "Laravel Temporary Tag simplify tagging Eloquent models.",
    "type": "library",
    "license": "MIT",
    "version": "v1.0.0",
    "require": {
        "php": ">=7.1.3",
        "laravel/framework": "^11.9"
    },
    "autoload": {
        "files" : [
            "src/helpers.php"
        ],
        "psr-4": {
            "Imanghafoori\\Tags\\": "src/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Imanghafoori\\Tags\\TempTagServiceProvider"
            ]
        }
    }
}
